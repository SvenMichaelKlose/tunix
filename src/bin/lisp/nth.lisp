(fn nth (n l)
  (and (<= 0 n)
    (? (or (not l)
           (== n 0))
       (car l)
       (nth (-- n) (cdr l)))))

(message "Testing NTH...")
(and (nth -1 '(l i s p))
     (error))
(or (equal (nth 0 '(l i s p))
           'l)
    (error))
(or (equal (nth 2 '(l i s p))
           's)
    (error))
(and (nth 4 '(l i s p))
     (error))
